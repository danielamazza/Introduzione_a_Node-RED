    {
        "id": "91d956e2.1c3c48",
        "type": "function",
        "z": "818e82ff.8b81f8",
        "name": "responses_to_chart_var",
        "func": "var iot = flow.get(\"iot\") || 0; \nvar game = flow.get(\"game\") || 0; \nvar web = flow.get(\"web\") || 0; \nvar scraping = flow.get(\"scraping\") || 0; \nvar geo = flow.get(\"geo\") || 0; \nvar chat = flow.get(\"chat\") || 0; \nvar social = flow.get(\"social\") || 0; \nvar quiz = flow.get(\"quiz\") || 0; \nvar stl = flow.get(\"stl\") || 0; \nvar flight = flow.get(\"flight\") || 0;\nif (msg.payload.iot) { iot+=parseInt(msg.payload.iot);} \nif (msg.payload.game) { game+=parseInt(msg.payload.game);} \nif (msg.payload.web) { web+=parseInt(msg.payload.web);}\nif (msg.payload.scraping) { scraping+=parseInt(msg.payload.scraping);} \nif (msg.payload.geo) { geo+=parseInt(msg.payload.geo);}\nif (msg.payload.chat) { chat+=parseInt(msg.payload.chat);} \nif (msg.payload.social) { social+=parseInt(msg.payload.social);}\nif (msg.payload.quiz) { social+=parseInt(msg.payload.quiz);}\nif (msg.payload.stl) { social+=parseInt(msg.payload.stl);}\nif (msg.payload.flight) { flight+=parseInt(msg.payload.flight);}\n\nflow.set([\"iot\",\"game\",\"web\",\"scraping\",\"geo\",\"chat\",\"social\",\"quiz\",\"stl\",\"flight\"],[iot,game,web,scraping,geo,chat,social,quiz,stl,flight])\nmsg.payload[\"series\"]= [\"iot\", \"game\",\"web\", \"scraping\",\"geo\",\"chat\",\"social\",\"quiz\",\"stl\",\"flight\"];\nmsg.payload[\"data\"]  = [[iot],[game],[web],[scraping],[geo],[chat],[social],[quiz],[stl],[flight]];\nmsg.payload=[{\n    \"series\": [\"\", \"\", \"\",\"\",\"\",\"\"],\n    \"data\": [[iot,game,web,scraping,geo,chat,social,quiz,stl,flight]],\n    \"labels\": [ \"iot\",\"game\", \"web\", \"scraping\",\"geo\",\"chat\",\"social\",\"quiz\",\"stl\",\"flight\" ]\n}];\nreturn msg;",
